    <!--Prebuilts -->
    <project path="prebuilts/abi-dumps/vndk" name="prebuilts_abi-dumps_vndk" remote="temp" clone-depth="1" />
    <project path="prebuilts/tools-xtended" name="prebuilts_tools-xtended" remote="temp" clone-depth="1" />
